{
  "expo": {
    "scheme": "myapp",
    "description": "A demo CVP-Worker app",
    "name": "CVP Worker",
    "icon": "./assets/icon/CIC_logo_icon.png",
    "userInterfaceStyle": "automatic",
    "web": {
      "bundler": "metro"
    },
    "orientation": "portrait",
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": true,
        "NSFaceIDUsageDescription": "Allow $(PRODUCT_NAME) to use Face ID",
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to use Camera"
      },
      "icon": "./assets/icon/CIC_logo_icon.png",
      "splash": {
        "image": "./assets/icon/CIC_logo.svg.png",
        "resizeMode": "contain",
        "backgroundColor": "#e5e5e5"
      },
      "bundleIdentifier": "com.user9510.cvpworkerexporouterapp"
    },
    "android": {
      "splash": {
        "image": "./assets/icon/CIC_logo.svg.png",
        "resizeMode": "contain",
        "backgroundColor": "#e5e5e5"
      },
      "icon": "./assets/icon/CIC_logo_icon.png",
      "package": "com.user9510.cvpworkerexporouterapp"
    },

    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c88aa11d-10d6-4d86-aab6-3c53271651c2"
    }
  },
  "slug": "cvp-worker",
  "plugins": [
    [
      "expo-local-authentication",
      {
        "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
      }
    ],
    [
      "expo-updates",
      {
        "username": "user9510"
      }
    ]
  ],
  "locales": {
    "en": "./languages/english.json",
    "zhHK": "./languages/traditionalChinese.json",
    "zhCN": "./languages/simplifiedChinese.json"
  },
  "android": {
    "package": "com.user9510.cvpworkerexporouterapp"
  }
}
